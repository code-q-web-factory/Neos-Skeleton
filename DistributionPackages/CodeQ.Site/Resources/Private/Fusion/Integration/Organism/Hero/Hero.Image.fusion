prototype(CodeQ.Site:Component.Hero.Image) < prototype(Neos.Fusion:Component) {
	# api
	image = false

	# rendering
    renderer = afx`
		<section class="section hero">
			<CodeQ.Site:Component.Image.Fullscreen
				class="hero__image"
				lazy={false}
				imageSource={props.image}
				backgroundPreviewImage={props.image}
				alt=""
				dataObjectFit="cover"
				/>
		</section>
    `
}
