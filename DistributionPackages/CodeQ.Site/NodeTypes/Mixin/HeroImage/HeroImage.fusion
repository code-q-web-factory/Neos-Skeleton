prototype(CodeQ.Site:Mixin.HeroImage) < prototype(Neos.Fusion:Component) {
	# api
    imageSource = CodeQ.Site:Integration.Helper.AssetImageSource {
        image = ${q(node).property('heroImage')}
    }

	# rendering
    renderer = CodeQ.Site:Presentation.Organism.HeroImage {
        @if.has = ${props.imageSource}
        @apply.props = ${props}
    }
}
