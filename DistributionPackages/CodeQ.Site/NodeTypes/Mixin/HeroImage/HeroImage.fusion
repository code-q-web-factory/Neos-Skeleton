prototype(CodeQ.Site:Mixin.HeroImage) < prototype(Neos.Fusion:Component) {
	# api
	imageSource = ${q(node).property('heroImage')}
    imageSource.@process.convert = CodeQ.Site:Integration.Helper.ImageAssetToSource

	# rendering
    renderer = CodeQ.Site:Presentation.Organism.HeroImage {
        @apply.props = ${props}
    }
}
