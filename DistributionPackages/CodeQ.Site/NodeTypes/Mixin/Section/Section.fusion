prototype(CodeQ.Site:Mixin.Section) < prototype(Neos.Fusion:Component) {
    # api
    class = null
    paddingY = 'medium'
    content = null

    # default integration
    id = CodeQ.JumpMarkers:NodeAnchorId
    backgroundColor = ${q(node).property('sectionBackgroundColor') || 'white'}
    spaceBelow = ${q(node).property('spaceBelow') || 'none'}

    renderer = afx`
        <CodeQ.Site:Presentation.Organism.Section {...props}/>
    `
}
