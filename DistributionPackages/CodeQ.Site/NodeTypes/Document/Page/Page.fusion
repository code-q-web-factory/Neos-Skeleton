prototype(CodeQ.Site:Document.Page) < prototype(CodeQ.Site:Document.AbstractPage) {
    body.content = Neos.Fusion:Component {
        main = Neos.Neos:ContentCollection {
            attributes.class = 'main'
            nodePath = 'main'
            tagName = 'main'
        }

        renderer = afx`
            {props.main}
        `
    }

    @cache {
        mode = 'uncached'
        context {
            1 = 'site'
            2 = 'node'
            3 = 'documentNode'
        }
    }
}
