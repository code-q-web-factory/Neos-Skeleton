prototype(CodeQ.Site:Content.Image.Block) < prototype(Neos.Neos:ContentComponent) {
    class = CodeQ.Site:Mixin.SpaceBelow
    imageSource = ${q(node).property('image')}
    imageSource.@process.convert = CodeQ.Site:Integration.Helper.ImageAssetToSource
    alternativeText = ${q(node).property('alternativeText')}

    renderer = afx`
        <CodeQ.Site:Presentation.Atom.Image.Auto @if.has={props.imageSource} {...props}/>
	`
}
